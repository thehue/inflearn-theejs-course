{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "no-console": 1, // Remember, this means warning!
    "@typescript-eslint/no-var-requires": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
}
